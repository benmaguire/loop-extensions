+//
+//  MedicalDeviceLockScrWidget.swift
+//  MedicalDeviceLockScrWidget
+//
+//  Created by Ben Maguire on 20/4/2025.
+//  Copyright © 2025 LoopKit Authors. All rights reserved.
+//
+
+import WidgetKit
+import SwiftUI
+
+struct Provider: TimelineProvider {
+    func placeholder(in context: Context) -> SimpleEntry {
+        SimpleEntry(date: Date())
+    }
+
+    func getSnapshot(in context: Context, completion: @escaping (SimpleEntry) -> ()) {
+        let entry = SimpleEntry(date: Date())
+        completion(entry)
+    }
+
+    func getTimeline(in context: Context, completion: @escaping (Timeline<Entry>) -> ()) {
+        var entries: [SimpleEntry] = []
+
+        // Generate a timeline consisting of five entries an hour apart, starting from the current date.
+        let currentDate = Date()
+        for hourOffset in 0 ..< 5 {
+            let entryDate = Calendar.current.date(byAdding: .hour, value: hourOffset, to: currentDate)!
+            let entry = SimpleEntry(date: entryDate)
+            entries.append(entry)
+        }
+
+        let timeline = Timeline(entries: entries, policy: .atEnd)
+        completion(timeline)
+    }
+}
+
+struct SimpleEntry: TimelineEntry {
+    let date: Date
+}
+
+
+struct MedicalDeviceLockScrWidgetEntryView : View {
+    var entry: Provider.Entry
+
+    var body: some View {
+        VStack {
+            Image("FirstAidKitEmoji")
+                .resizable()
+                .frame(width: 25, height: 35)
+            Text("MEDICAL DEVICE")
+                .font(.caption)
+                .bold()
+            Text("Type 1 Diabetic")
+                .font(.custom("SFUI-Regular", fixedSize: 10))
+                .foregroundColor(.gray)
+        }
+        .alignmentGuide(.customCenter) { context in
+          context.width / 4
+        }
+        
+    }
+}
+
+struct MedicalDeviceLockScrWidget: Widget {
+    let kind: String = "MedicalDeviceLockScrWidget"
+
+    var body: some WidgetConfiguration {
+        StaticConfiguration(kind: kind, provider: Provider()) { entry in
+            if #available(iOS 17.0, *) {
+                MedicalDeviceLockScrWidgetEntryView(entry: entry)
+                    .containerBackground(.fill.tertiary, for: .widget)
+            } else {
+                MedicalDeviceLockScrWidgetEntryView(entry: entry)
+                    .padding()
+                    .background()
+            }
+        }
+        
+        .supportedFamilies([
+            .systemSmall,
+            .accessoryInline,
+            .accessoryRectangular
+        ])
+        
+        .configurationDisplayName("Medical Device Label")
+        .description("This widget shows a label for your medical device")
+    }
+}
+
+#Preview(as: .systemSmall) {
+    MedicalDeviceLockScrWidget()
+} timeline: {
+    SimpleEntry(date: .now)
+}
+
+
+
+struct CustomCenter: AlignmentID {
+  static func defaultValue(in context: ViewDimensions) -> CGFloat {
+    context[HorizontalAlignment.center]
+  }
+}
+
+
+struct CustomRight: AlignmentID {
+  static func defaultValue(in context: ViewDimensions) -> CGFloat {
+      context[HorizontalAlignment.trailing]
+  }
+}
+
+extension HorizontalAlignment {
+    static let customCenter: HorizontalAlignment = .init(CustomCenter.self)
+    static let customRight: HorizontalAlignment = .init(CustomRight.self)
+}
+
